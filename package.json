{
  "name": "typescript-api-automation-vitest",
  "version": "1.0.0",
  "description": "REST API Automation Framework with TypeScript, Axios, Vitest, and Yarn",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:report": "vitest run --reporter=html",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "api-testing",
    "automation",
    "typescript",
    "vitest",
    "axios",
    "rest-api",
    "testing-framework"
  ],
  "author": "sanduniDev",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  }
}